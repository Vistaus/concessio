<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="CncWindow">
        <property name="title" translatable="true">Concessio</property>
        <property name="width-request">360</property>
        <property name="height-request">400</property>
        <child>
            <object class="AdwBreakpoint">
                <condition>max-width: 550sp</condition>
                <setter object="header_bar" property="title-widget"></setter>
                <setter object="switcher_bar" property="reveal">true</setter>
            </object>
        </child>
        <property name="content">
            <object class="GtkBox">
                <property name="orientation">1</property>
                <child>
                    <object class="AdwHeaderBar" id="header_bar">
                        <property name="title-widget">
                            <object class="AdwViewSwitcher">
                                <property name="stack">viewStack</property>
                                <property name="policy">1</property>
                            </object>
                        </property>
                    </object>
                </child>
                <child>
                    <object class="AdwViewStack" id="viewStack">
                        <property name="vexpand">true</property>
                        <child>
                            <object class="AdwViewStackPage" id="simple">
                                <property name="name">simple</property>
                                <property name="title" translatable="true">Simple</property>
                                <property name="icon-name">star-filled-rounded-symbolic</property>
                                <property name="use-underline">true</property>
                                <property name="child">
                                    <object class="GtkLabel">
                                        <property name="label" bind-source="simple"
                                            bind-property="title" bind-flags="sync-create" />
                                    </object>
                                </property>
                            </object>
                        </child>
                        <child>
                            <object class="AdwViewStackPage" id="symbolic">
                                <property name="name">symbolic</property>
                                <property name="title" translatable="true">Symbolic</property>
                                <property name="icon-name">clock-alt-symbolic</property>
                                <property name="use-underline">true</property>
                                <property name="child">
                                    <object class="GtkLabel">
                                        <property name="label" bind-source="symbolic"
                                            bind-property="title" bind-flags="sync-create" />
                                    </object>
                                </property>
                            </object>
                        </child>
                        <child>
                            <object class="AdwViewStackPage" id="numeric">
                                <property name="name">numeric</property>
                                <property name="title" translatable="true">Numeric</property>
                                <property name="icon-name">bell-outline-symbolic</property>
                                <property name="badge-number">0</property>
                                <property name="needs-attention">true</property>
                                <property name="use-underline">true</property>
                                <property name="child">
                                    <object class="GtkLabel">
                                        <property name="label" bind-source="numeric"
                                            bind-property="title" bind-flags="sync-create" />
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="AdwViewSwitcherBar" id="switcher_bar">
                        <property name="stack">viewStack</property>
                    </object>
                </child>
            </object>
        </property>
    </template>
</interface>